               </persName>
                    <lb n="4"/>patronae bene
                    <lb n="5"/>merenti fecit <expan>
                  <abbr>h</abbr><ex>onoris</ex>
               </expan> 
               <expan>
                  <abbr>c</abbr><ex>ausa</ex></expan>.
            </ab>
