        <revisionDesc>
            <change when="2018-06-24" who="Sunju Li">Initial Encoding</change>
            <change when="2019-07-16" who="SJD">Cleaned and added metadata, dimension; added location/provenance info; added bibliography entries; added image filepath; proofed and made very minor changes to transcription</change>
            <change when="2019-08-09" who="SJD">Fixed invalid pieces of provenance/dating</change>
        </revisionDesc>

    </teiHeader>

