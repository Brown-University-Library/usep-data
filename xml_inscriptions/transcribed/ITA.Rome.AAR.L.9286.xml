        <encodingDesc>
                           
                <xi:include
                    href="http://library.brown.edu/usep_data/resources/include_taxonomies.xml">
                    <xi:fallback>
                        <p>Taxonomies for US Epigraphy controlled values</p>
                    </xi:fallback>
                </xi:include>
            
        </encodingDesc>

        <profileDesc> <!-- not used --> </profileDesc>
        
        
        <!--  *************************************  <revisionDesc>  ********************************* -->
