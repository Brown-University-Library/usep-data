                <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> undiqu<supplied reason="lost">e</supplied> <gap reason="lost" atMost="1" unit="character"/> <unclear>OI</unclear> <gap reason="lost" extent="unknown" unit="character"/>"
                <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost" cert="low">i</supplied>den qui patir <unclear>n</unclear><supplied reason="lost" cert="low">oster</supplied> <gap reason="lost" extent="unknown" unit="character"/>
                <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/> cu<hi rend="small">m</hi> <hi rend="tall">l</hi>audibus <unclear>F</unclear> <gap reason="lost" extent="unknown" unit="character"/>.
