         <div type="edition"><!-- transcription goes here -->
            <ab>
               <lb n="1"/>D<hi rend="tall">i</hi>s <expan><abbr>Man</abbr><ex>ibus</ex></expan>
               <lb n="2"/>
               <persName>
                  <name type="gentilicium" key="Hygia">H<hi rend="tall">y</hi>
                     <hi rend="apex">g</hi>i<hi rend="apex">a</hi>e</name>
                  <name type="cognomen" key="Stratonice">Stratonices</name>
               </persName>
               <lb n="3"/>Tra<hi rend="apex">l</hi>ia<hi rend="apex">n</hi>ae. <expan><abbr>Vix</abbr><ex>it</ex></expan> a<hi rend="apex">n</hi>nis
                <lb n="4"/>
               <num value="18">X<hi rend="apex">V</hi>III</num>. <persName>
                  <name type="cognomen" key="Plocamus">Pl<hi rend="apex">o</hi>ca<hi rend="apex">m</hi>us</name>
               </persName> coniug<hi rend="tall">i</hi>
               <lb n="5"/>bene de <hi rend="hi">s</hi>e me<hi rend="apex">r</hi>itae
                <lb n="6"/>
               <hi rend="tall">f</hi>ecit.
            </ab>
