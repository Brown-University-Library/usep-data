               <msContents>
                  <textLang mainLang="lat"/>
                  <!-- arc, grc, he, lat, und (unknown),  zxx (non-linguistic content) -->
                  <msItem class="#funerary.epitaph">
                        <!-- short summary of the content of an inscription -->
                     <p>Epitaph of Julia, commemorating numerous individuals</p>
                  </msItem>
               </msContents>
               <!--  *************************************  <physDesc>  ********************************* -->
               <physDesc>

                        <!--  *************************************  <objectDesc>  *********************************
                            <objectDesc> contains information about the physical properties of the object and the inscribed text.  -->
                  <objectDesc ana="#stele">
                     <supportDesc ana="#stone.marble">
                        <support>
                        <!-- short summary of the material and support of an inscription -->
                           <p>Marble stele</p>
                           <dimensions type="surface" unit="cm">
                              <height>76.2</height>
                              <width>33.6</width>
                              <depth/>
                           </dimensions>
                        </support>
                        <condition ana="#complete.broken">
                           <p>inscription complete but so badly weathered on right hand side that much of it is illegible</p>
                        </condition>
                     </supportDesc>
                     <!--  *************************************  <layoutDesc>  ********************************* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="20">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!--  *************************************  <handDesc>  ********************************* -->
                  <!--     Contains information about types of writing and means of inscription.  -->
                  <handDesc hands="1">
                     <handNote ana="#inscribed">
                        <dimensions type="letter" unit="cm">
                           <height min="2.0" max="3.0">2.0-3.0</height>
                        </dimensions>
                     </handNote>
                     <!-- <handNote> is a repeatable element -->
                  </handDesc>
                  <!--  *************************************  <decoDesc>  *********************************     -->
                  <decoDesc>
                     <decoNote>
                        <p>above campus, human figure in rounded frame</p>
                     </decoNote>
                  </decoDesc>
               </physDesc>
               <!--  *************************************  <history>  *********************************  -->
               <!-- <origin> encodes the origin of inscription including place and date.
                         <date> use 4 digit years, with leading zeros. BCE has a - sign in front of the number. Both @notBefore
                           and @notAfter are required. There is no 0000 date;  use -0001 or 0001.
                    -->
               <history>
                  <summary>
                     <rs/>
                     <!-- content can be "modern" or "modern-text". A blank value indicates that the inscription is ancient.  -->
                  </summary>
                  <origin>
                            <!-- original place and dating of inscription -->
                     <date evidence="#formulae #nomenclature" notBefore="0075" notAfter="0200">late
                                first-second century CE</date>
                     <placeName ref="unknown">unknown</placeName>
                  </origin>
               </history>
            </msDesc>
            <!--  *************************************  <listBibl>  *********************************  -->
