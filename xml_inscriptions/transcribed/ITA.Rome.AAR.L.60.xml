                        <biblScope unit="page">107-110, no. 2</biblScope>
                    </bibl>
                    <bibl>
                        <ptr type="rest-of-bibl" target="#Marucchi_1933"/>
                        <biblScope unit="page">310-311</biblScope>
                    </bibl>
                    <bibl>
                        <ref type="external" target="http://nvb.aarome.org/scheda.cfm?scheda=1&amp;id=1087&amp;object=&amp;culture=&amp;source1=&amp;description1=&amp;material1=&amp;provenance1=&amp;category1=&amp;tipo=Inscriptions">AAR Catalog Entry</ref>
                    </bibl>
                </listBibl>
            </sourceDesc>
        </fileDesc>

