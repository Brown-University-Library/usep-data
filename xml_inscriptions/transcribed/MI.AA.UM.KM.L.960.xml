               <persName>
                  <name type="cognomen" key="Nicephorus">Nicephorus</name>
               </persName> et <persName>
                  <name type="gentilicium" key="Ulpia">Ulpia</name> 
                  <name type="cognomen" key="Profutura">Pro
                <lb break="no" n="6"/>futura</name>
