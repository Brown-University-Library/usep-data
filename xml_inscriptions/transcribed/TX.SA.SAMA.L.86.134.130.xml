                    <lb n="1"/><expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan>. <persName><name type="praenomen" key="Titus"><expan><abbr>T</abbr><ex>ito</ex></expan></name> <name type="gentilicium" key="Flavius">Flavio</name>
                    <lb n="2"/><name type="cognomen" key="Hierax"><expan><ex>H</ex><abbr>ieraci</abbr></expan></name></persName> <surplus>f</surplus> <persName><name type="cognomen" key="Eutychia">Eut
                    <lb n="3" break="no"/><hi rend="tall">y</hi><choice><corr>c</corr><sic>o</sic></choice>hia</name></persName> coiugi
                    <lb n="4"/><expan><abbr>b</abbr><ex>ene</ex></expan> <expan><abbr>m</abbr><ex>erenti</ex></expan> <expan><abbr>f</abbr><ex>ecit</ex></expan>.
                </ab>
