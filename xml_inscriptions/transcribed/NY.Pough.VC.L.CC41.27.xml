                    <lb n="2"/><persName><name type="praenomen" key="Lucius"><expan><abbr>L</abbr><ex>ucio</ex></expan></name> <name type="gentilicium" key="Vitellius">Vitellio</name> <name type="cognomen" key="Leo">Leoni</name></persName> filio
