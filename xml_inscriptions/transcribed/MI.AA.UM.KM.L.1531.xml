                     <expan>
                        <abbr><choice><sic>E</sic><corr>F</corr></choice>loren<hi rend="ligature">ti</hi>nu</abbr><ex>s</ex>
                     </expan>
                  </name></persName>
                <lb n="2"/> vixit <expan>
                  <abbr>ann</abbr><ex>is</ex>
