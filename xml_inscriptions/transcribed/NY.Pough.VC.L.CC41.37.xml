    <text>
        <body>
            <div type="edition"><!-- transcription goes here -->
                <div type="textpart" subtype="column" n="1">
                    <ab>
                        <lb n="1"/><persName><name type="gentilicium" key="Volcelia">Volcelia</name> 
                        <lb n="2"/><persName><name type="praenomen" key="Marcus"><expan><abbr>M</abbr><ex>arci</ex></expan></name></persName> <expan><abbr>l</abbr><ex>iberta</ex></expan>
                        <lb n="3"/><name type="cognomen" key="Fausta">Fausta</name></persName>.
                    </ab>
                </div>
                <div type="textpart" subtype="column" n="2">
                    <ab>
                        <lb n="1"/><persName><name type="gentilicium" key="Vergilia">Vergilia</name>
                        <lb n="2"/><persName><g type="mulieris"/></persName> <expan><abbr>l</abbr><ex>iberta</ex></expan>
                        <lb n="3"/><name type="cognomen" key="Prima">Prima</name></persName>.
                    </ab>
                </div>
            </div>
             
            <div type="translation"><!-- notes go here -->
                <div type="textpart" subtype="column" n="1">
                    <p>"Volcelia Fausta, freedwoman of Marcus."</p>
                </div>
                <div type="textpart" subtype="column" n="2">
                    <p>"Vergilia Prima, freedwoman of a woman."</p>
                </div>                   
            </div>
            
