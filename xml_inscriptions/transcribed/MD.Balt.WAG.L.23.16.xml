                                </condition>
                            </supportDesc>
                            <!--  *************************************  <layoutDesc>  ********************************* -->
                            <layoutDesc>
                                <layout columns="1" writtenLines="8">
                                    <p/>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <!--  *************************************  <handDesc>  ********************************* -->
