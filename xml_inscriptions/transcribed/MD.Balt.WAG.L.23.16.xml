                        <decoDesc>
                            <decoNote>
                                <p>Ring on right side, unfinished vase on left side</p>
                            </decoNote>
                        </decoDesc>
                    </physDesc>
                    
                    <!--  *************************************  <history>  *********************************  -->
                    
                    <!-- <origin> encodes the origin of inscription including place and date. 
                         <date> use 4 digit years, with leading zeros. BCE has a - sign in front of the number. Both @notBefore 
                           and @notAfter are required. There is no 0000 date;  use -0001 or 0001.
                    -->
                    <history>
                    
                        <summary>
                            <rs/>  <!-- content can be "fake" or blank -->
                        </summary>
                        
                        <origin>
                            <!-- original place and dating of inscription -->
                            <date evidence="unspecified" notBefore="0001" notAfter="0100">1st century CE</date>
                            <placeName ref="europe.italy.rome">Vinea Brunona, Via Appia</placeName>
                        </origin>
                        <provenance>
                            
                            <p>In the <persName>Don Marcello Massarenti</persName> Collection, <placeName>Rome</placeName> <date notBefore="1850" notAfter="1902">before 1902
                            </date></p>
                        </provenance>
<provenance type="transferred" subtype="purchased">
    
    <p>Purchased by <persName>Henry Walters</persName> in <date when="1902">1902
    </date>
    </p>
</provenance>
                        <provenance type="transferred" subtype="given">
