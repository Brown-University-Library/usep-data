                        <objectDesc ana="#stele">
                            <supportDesc ana="#stone.marble">
                                <support>
                                    <!-- short summary of the material and support of an inscription -->
                                    <p> Marble stele, clamp holes on top of each end; base and top
                                        not part of original inscription</p>
                                    <dimensions type="surface" unit="cm">
                                        <height>113</height>
                                        <width>66.5</width>
                                        <depth>55.7</depth>
                                    </dimensions>
                                </support>
                                <condition ana="#complete.intact">
