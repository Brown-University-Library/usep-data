               </bibl>
               <bibl>
                  <ref type="external"
                        target="http://www.edr-edr.it/edr_programmi/res_complex_comune.php?do=book&amp;id_nr=EDR146555">EDR146555</ref>
               </bibl>
               <bibl>
                  <ref type="external"
                        target="http://nvb.aarome.org/scheda.cfm?scheda=1&amp;id=1117&amp;object=&amp;culture=&amp;source1=&amp;description1=&amp;material1=&amp;provenance1=&amp;category1=&amp;tipo=Inscriptions">AAR Catalog Entry</ref>
               </bibl>
            </listBibl>
         </sourceDesc>
      </fileDesc>
