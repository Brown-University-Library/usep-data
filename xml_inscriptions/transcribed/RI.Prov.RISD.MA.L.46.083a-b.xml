                  </summary>
                  <origin>
                            <!-- original place and dating of inscription -->
                     <date evidence="unspecified" notBefore="0041" notAfter="0069">mid-first century CE (late
                                Claudian to Neronian date)</date>
                     <placeName ref="europe.italy.rome">Italy, Rome, Via Ostiense, Tomb 4E at the necropolis near the
                                basilica of St. Paul</placeName>
                  </origin>
                  <provenance type="found">
                     <p>
                        <persName>Lugli</persName> discovered and published the urn in <date when="1919">1919</date>
                     </p>
                  </provenance>
                  <provenance type="transferred" subtype="loaned">
                     <p>Loaned by <persName>Marshall H. Gould</persName> to RISD in <date when="1926">1926</date>
                     </p>
                  </provenance>
                  <provenance type="transferred" subtype="given">
                     <p>Permanently given to RISD by <persName>Marshall H. Gould</persName> in <date when="1946">1946</date>
                     </p>
                  </provenance>
                  <!-- subsequent locations if inscription was moved -->
               </history>
            </msDesc>
