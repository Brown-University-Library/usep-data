            <ab>
               <lb n="1"/>
               <gap reason="lost" extent="unknown" unit="character"/>
               <unclear>i</unclear>a <persName>
                  <name type="cognomen" key="Eutyches">Eutycheti<supplied reason="lost">s</supplied>
                  </name>
               </persName>
