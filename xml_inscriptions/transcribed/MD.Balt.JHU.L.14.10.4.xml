                  <msItem class="#label">
                            <!-- short summary of the content of an inscription -->
                     <p>Stamped lead pipe recording the name Eutyches.</p>
                  </msItem>
               </msContents>
               <!--  *************************************  <physDesc>  ********************************* -->
               <physDesc>
                    
                        <!--  *************************************  <objectDesc>  *********************************
                            <objectDesc> contains information about the physical properties of the object and the inscribed text.  -->
                  <objectDesc ana="#instrumentum.other">
                     <supportDesc ana="#metal.lead">
                        <support>
                                    <!-- short summary of the material and support of an inscription -->
                           <p>Lead fistula</p>
                           <dimensions type="surface" unit="cm">
                              <height>10.5</height>
                              <width>36</width>
                              <depth>6</depth>
                           </dimensions>
                        </support>
                        <condition ana="#fragment.single">
                           <p>The pipe has been broken at the right and at the left, and
                                        has some light weathering on the surface, but it is
                                        comparatively in fair shape with no cracks or ruptures</p>
                        </condition>
                     </supportDesc>
                     <!--  *************************************  <layoutDesc>  ********************************* -->
                     <layoutDesc>
                        <layout columns="1" writtenLines="1">
                           <p/>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <!--  *************************************  <handDesc>  ********************************* -->
