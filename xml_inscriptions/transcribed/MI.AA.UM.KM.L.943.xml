               </expan> 
               <expan>
                  <abbr>Cl</abbr><ex>assis</ex>
               </expan> 
               <expan>
                  <abbr>Pr</abbr><ex>aetoriae</ex>
               </expan> 
               <expan>
                  <abbr>Misenen</abbr><ex>sis</ex>
               </expan>
                    <lb n="4"/>
                   <g type="triere"/> 
               <expan>
                  <abbr>Libertat</abbr><ex>e</ex>
               </expan> 
               <expan>
