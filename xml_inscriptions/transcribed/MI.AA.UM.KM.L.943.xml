                     <expan>
                        <abbr>L</abbr><ex>ucius</ex>
                     </expan>
                  </name> 
                  <name type="gentilicium" key="Sossius">Sossius</name> 
                  <name type="cognomen" key="Dionysius">Dionysius</name>
               </persName> 
                   <g type="triere"/> 
               <expan>
