                <ab>
                    <lb n="1"/>
               <expan>
                  <abbr>D</abbr><ex>is</ex>
               </expan> 
               <expan>
                  <abbr>M</abbr><ex>anibus</ex></expan>.
                    <lb n="2"/>
               <persName>
                  <name type="praenomen" key="Titus">
                     <expan>
                        <abbr>T</abbr><ex>ito</ex>
                     </expan>
                  </name> 
                  <name type="gentilicium" key="Laelius">Laelio</name> 
                  <name type="cognomen" key="Crispus">Crispo</name></persName>,
