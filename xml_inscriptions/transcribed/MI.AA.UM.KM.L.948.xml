                <ab>
               <lb n="1"/>
               <expan>
                  <abbr>D</abbr><ex>is</ex>
               </expan>
               <expan>
                  <abbr>M</abbr><ex>anibus</ex></expan>. <lb n="2"/>
               <persName>
                  <name type="gentilicium" key="Iulia">Iuliae</name>
                  <name type="cognomen" key="Longina">Longine</name></persName>, <expan>
                  <abbr>q</abbr><ex>uae</ex>
               </expan>
               <lb n="3"/> vixit <expan>
                  <abbr>
                     <hi rend="supraline">an</hi></abbr><ex>nis</ex>
               </expan>
               <num xml:lang="lat" value="20">XX</num>, <persName>
                  <name type="gentilcium" key="Iulia">Iulia<sic>e</sic></name>
                  <lb n="4"/>
                  <name type="cognomen" key="Masa">Mas<sic>e</sic></name>
               </persName> et <persName>
                  <name type="gentilicium" key="Iulius">Iulius</name>
                  <name type="cognomen" key="Marinus">Ma <lb n="5" break="no"/>rinus</name>
                   </persName> parentes <expan><abbr>f<am>f</am></abbr><ex>ecerunt</ex></expan>, <lb n="6"/> et <persName>
                  <name type="gentilicium" key="Iulius">Iulius</name>
                  <name type="cognomen" key="Valens">Vale<supplied reason="omitted"
                     >n</supplied>s</name>
               </persName>
               <lb n="7"/> coniugi <expan>
                  <abbr>b</abbr><ex>ene</ex>
               </expan>
               <expan>
                  <abbr>m</abbr><ex>erenti</ex>
               </expan>
               <expan>
                  <abbr>f</abbr><ex>ecit</ex></expan>.</ab>
