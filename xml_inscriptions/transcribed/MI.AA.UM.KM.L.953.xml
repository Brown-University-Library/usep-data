                  <lb n="3"/>
                  <name type="cognomen" key="Celsus">Celso</name></persName>; vixit 
                   <lb n="4"/>
               annis <num xml:lang="lat" value="16">XVI</num>, <persName>
                  <name type="praenomen" key="Publius">
                     <expan>
                        <abbr>P</abbr><ex>ublius</ex>
                     </expan>
                  </name>
               </persName>
               <lb n="5"/>
               <persName>
                  <name type="praenomen" key="Naevius">
                     <choice>
                        <sic>V</sic>
                        <corr>N</corr>
                     </choice>aevius</name>
                  <name type="cognomen" key="Asper">Asper</name>
               </persName> fi 
                   <lb n="6" break="no"/> lio dulcissimo 
                   <lb n="7"/> fecit.</ab>
