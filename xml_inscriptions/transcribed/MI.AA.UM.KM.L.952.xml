                    <lb n="4"/>
               <persName>
                  <name type="gentilicium" key="Barbius">Barbius</name> 
                  <name type="cognomen" key="Firmus">Firmus</name>
               </persName> faber
                    <lb n="5"/>
               <expan>
                  <abbr>dupl</abbr><ex>icarius</ex>
               </expan> 
               <g type="tetrere"/> Dacico <expan>
                  <abbr>patro</abbr><ex>nus</ex>
