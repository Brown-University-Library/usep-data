               <num xml:lang="lat" value="12">XII</num>. <persName><name type="gentilicium"
                     key="Publicius">Publicius</name>
                  <lb n="5"/><name type="cognomen" key="Ianuarius">Ianu<supplied reason="omitted"
                        >a</supplied>rius</name></persName> alumno 
               <lb n="6"/>
               <expan>
                  <abbr>suo f</abbr><ex>e</ex><abbr>c</abbr><ex>it</ex></expan>.</ab>
                <!-- The marked up transcription goes here, surrounded by <ab> </ab> elements -->
                
