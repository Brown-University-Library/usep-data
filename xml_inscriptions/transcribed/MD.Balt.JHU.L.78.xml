                    <lb n="1"/><persName><name type="gentilicium" key="Flavia">Fl<hi rend="apex">a</hi>via</name> <persName><name type="praenomen" key="Decimus"><expan><abbr>D</abbr><ex>ecimi</ex></expan></name></persName> <expan><abbr>l</abbr><ex>iberta</ex></expan> <name type="cognomen" key="Salvia">Salvia</name></persName>. <hi rend="superscript"><expan><abbr>Oll</abbr><ex>ae</ex></expan> <num value="2">II</num></hi>.
                            <lb n="2"/>Patrono dat <expan><abbr>olla</abbr><ex>m</ex></expan> <expan><abbr>t</abbr><ex>itulum</ex></expan> <expan><abbr>d</abbr><ex>e</ex></expan> <expan><abbr>s</abbr><ex>ua</ex></expan> <expan><abbr>p</abbr><ex>ecunia</ex></expan>
