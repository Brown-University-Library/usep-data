        <div type="edition"><!-- transcription goes here -->
            <ab><lb n="1"/><expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan>.
            <lb n="2"/><persName><name type="praenomen" key="Lucius"><expan><abbr>L</abbr><ex>ucio</ex></expan></name> <name type="gentilicium" key="Titius">Titio</name> <persName><name type="praenomen" key="Lucius"><expan><abbr>L</abbr><ex>uci</ex></expan></name></persName> <expan><abbr>f</abbr><ex>ilio</ex></expan> <name type="cognomen" key="Iulianus"><expan><abbr>Iul</abbr><ex>iano</ex></expan></name> <name type="cognomen" key="Celer">Celeri</name> <name type="agnomen" key="Carnicus">Car
                <lb n="3" break="no"/>nico</name></persName>, <w lemma="speculator"><expan><abbr>sp</abbr><ex>eculatori</ex></expan></w> <expan><abbr>coh</abbr><ex>ortis</ex></expan> <num type="ordinal" value="9">VIIII</num> <expan><abbr>pr</abbr><ex>aetoriae</ex></expan>, <g type="centuriae"/> <persName><name type="cognomen" key="Montanus">Mon
                <lb n="4" break="no"/>tani</name></persName>. Vixit <expan><abbr>ann</abbr><ex>is</ex></expan> <num type="cardinal" value="34">XXXIIII</num>, <expan><abbr>m</abbr><ex>ensibus</ex></expan> <num type="cardinal" value="2">II</num>,
                <lb n="5"/><expan><abbr>d</abbr><ex>iebus</ex></expan> <num type="cardinal" value="7">VII</num>; <expan><abbr>mil</abbr><ex>itavit</ex></expan> annis <num type="cardinal" value="15">XV</num>. Fecit
                <lb n="6"/><persName><name type="praenomen" key="Lucius"><expan><abbr>L</abbr><ex>ucius</ex></expan></name> <name type="gentilicium" key="Titius">Titius</name> <name type="cognomen" key="Melancoma">Melancoma</name></persName> patrono
                <lb n="7"/><expan><abbr>b</abbr><ex>ene</ex></expan> <expan><abbr>m</abbr><ex>erenti</ex></expan>.
            </ab></div>
        
        
                <div type="translation">
