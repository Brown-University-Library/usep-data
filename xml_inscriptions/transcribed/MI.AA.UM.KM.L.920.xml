               <persName>
                  <name type="praenomen" key="Gaius">
                     <expan>
                        <abbr>C</abbr><ex>aio</ex>
                     </expan>
                  </name> 
                  <name type="gentilicium" key="Iulius">Iulio</name> 
                  <name type="cognomen" key="Carallitanus">Caralli
                    <lb n="3" break="no"/> tano</name>
               </persName> natione
                    <lb n="4"/> Italico. <expan>
                  <abbr>Vix<sic>s</sic></abbr><ex>it</ex>
               </expan> anni<hi rend="superscript">s</hi>
                    <lb n="5"/> 
               <num xml:lang="lat" value="15">XV</num>, <expan>
                  <abbr>m</abbr><ex>ensibus</ex>
               </expan> 
               <num xml:lang="lat" value="6">VI</num>, <persName>
                  <name type="gentilicium" key="Cassia">Cassia</name> 
                  <name type="cognomen" key="Fortunata">For
                    <lb n="6" break="no"/> tunata</name>
               </persName> filio <expan>
                  <abbr>b</abbr><ex>ene</ex>
               </expan> 
               <expan>
                  <abbr>m</abbr><ex>erenti</ex>
               </expan> 
               <expan>
                  <abbr>f</abbr><ex>ecit</ex></expan>.</ab>
