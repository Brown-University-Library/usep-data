            -->
            <titleStmt>
                <title>Auct.Christies.NYAnt.2012.6.8.Lot210</title>
            </titleStmt>
            <!-- *************************************  <publicationStmt>  *********************************
                <idno>: Repeat the value that is in <title>.
                @xml:id is the machine readable US Epigraphy number. Replace characters such as ? * # & +  [space]  with a set of predeterimined 
                characters that are legal in XML names and viable URLs. 
            -->
            <publicationStmt>
               
                <authority>Brown University</authority> 
                <idno type="USEpigraphy" xml:id="Auct.Christies.NYAnt.2012.6.8.Lot210">Auct.Christies.NYAnt.2012.6.8.Lot210</idno>
