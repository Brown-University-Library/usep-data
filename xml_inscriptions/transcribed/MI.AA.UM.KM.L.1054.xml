                     </expan> <expan><abbr>Fab</abbr><ex>ia</ex>
                     </expan>
                    <lb n="2"/> 
                     <name type="cognomen" key="Erastus">Erasti</name>
                  </persName> 
                  <expan>
                     <abbr>lib</abbr><ex>ertus</ex>
                  </expan> 
