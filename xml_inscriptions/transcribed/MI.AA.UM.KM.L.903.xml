               <lb n="4"/>
               <persName>
                  <name type="gentilicium" key="Gabina">Gabinia</name>
                  <name type="cognomen" key="Venusta">Venusta</name>
               </persName>
               <lb n="5"/>patrona<choice>
                  <corr>e</corr>
               </choice>
