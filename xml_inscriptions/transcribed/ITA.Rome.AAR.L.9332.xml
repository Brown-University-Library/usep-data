            <lb n="8"/><name type="cognomen" key="Faustinus"><hi rend="tall">F</hi>aust<hi rend="tall">i</hi>n<hi rend="tall">i</hi></name></persName> <expan><abbr>ser</abbr><ex>va</ex></expan> <expan><abbr>fec</abbr><ex>it</ex></expan> sibi <hi rend="ligature">et</hi>
