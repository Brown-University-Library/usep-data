                    <lb n="5"/>
               <num xml:lang="lat" value="25">XXV</num>. 
               <persName>
                  <name type="gentilicium" key="Aelia">Aelia</name> 
                  <name type="cognomen" key="Casta">Casta</name>
               </persName>
                    <lb n="6"/>mater <expan>
                  <abbr>b</abbr><ex>ene</ex>
               </expan> 
               <expan>
                  <abbr>m</abbr><ex>erenti</ex>
