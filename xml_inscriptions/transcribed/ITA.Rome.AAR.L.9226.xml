                                    <p>Marble funerary stele</p>
                                    <dimensions type="surface" unit="cm">
                                        <height>27.5</height>
                                        <width>14.5 (base of portrait); 13.5 (bottom); 27.5 (total)</width>
                                        <depth>4.4</depth>
                                    </dimensions>
                                </support>
                                <condition ana="#complete.intact">
                                    <p/>
                                </condition>
                            </supportDesc>
                            <!--  *************************************  <layoutDesc>  ********************************* -->
                            <layoutDesc>
                                <layout columns="1" writtenLines="7">
                                    <p/>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        
                        <!--  *************************************  <handDesc>  ********************************* -->
                      <!--     Contains information about types of writing and means of inscription.  -->
                        <handDesc hands="1">
                            <handNote ana="#impressed.inscribed.carved">
            <dimensions type="letter" unit="cm">
                                    <height min="1.3" max="2.7">1.3-2.7</height>
                                </dimensions>
            
            
            
            
            
                                
                            </handNote>
                            <!-- <handNote> is a repeatable element -->
                        </handDesc>
                        
                        <!--  *************************************  <decoDesc>  *********************************     -->
