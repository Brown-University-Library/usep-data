              <lb n="6"/><expan><abbr>ann</abbr><ex>is</ex></expan> <num type="cardinal" value="3">III</num> e<hi rend="tall">t</hi> <expan><abbr>men</abbr><ex>sibus</ex></expan> <num type="cardinal" value="3">III</num>, 
