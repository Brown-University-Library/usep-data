            <ab>
                <lb n="1"/><persName><name type="praenomen" key="Cnaeus"><expan><abbr>Cn</abbr><ex>aei</ex></expan></name> <name type="gentilicium" key="Antonius">Antoni</name> <name type="cognomen" key="Laudemedes">Laumedi</name></persName>,
                <lb n="2"/><persName><name type="gentilicium" key="Aemilia">Aemiliai</name> <g type="mulieris"/> <expan><abbr>l</abbr><ex>ibertai</ex></expan>, <name type="cognomen" key="Heliconia">Heliconiai</name></persName>,
                <lb n="3"/><persName><add place="overstrike"><name type="praenomen" key="Cnaeus"><expan><abbr>Cn</abbr><ex>aei</ex></expan></name> <name type="gentilicium" key="Antonius">Antoni</name></add> <persName><name type="praenomen" key="Cnaeus"><expan><abbr><add place="overstrike">C</add>n</abbr><ex>aei</ex></expan></name></persName> <expan><abbr>l</abbr><ex>iberti</ex></expan>
                <lb n="4"/><add place="overstrike"><name type="cognomen" key="Moschio">Moschioni</name></add></persName>.
            </ab></div>
        
        
