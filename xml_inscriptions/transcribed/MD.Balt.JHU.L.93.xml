        <div type="edition"><!-- transcription goes here -->
                <ab>
                    <lb n="1"/><expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan>.
                    <lb n="2"/><persName><name type="praenomen" key="Caius"><expan><abbr>C</abbr><ex>aio</ex></expan></name> <name type="gentilicium" key="Iulius">Iulio</name> <name type="cognomen" key="Saturninus">Satu<unclear cert="high">r</unclear>
                    <lb n="3" break="no"/>nino</name></persName> coniugi 
                    <lb n="4"/>karissimo <persName><name type="gentilicium" key="Pullaena">Pu<supplied reason="lost" cert="low">l</supplied>
                    <lb n="5"/>laena</name> <name type="cognomen" key="Vema">Vem<supplied reason="lost" cert="low">a</supplied></name></persName> <supplied reason="lost" cert="low"><expan><abbr>f</abbr><ex>ecit</ex></expan></supplied>
                    <lb n="6"/><supplied reason="undefined" evidence="previouseditor"><gap reason="illegible" quantity="6" unit="character"/></supplied>
                </ab></div>
        
        
