               <expan>
                  <abbr>M</abbr><ex>anibus</ex></expan>. <lb n="2"/>
               <persName>
                  <name type="praenomen" key="Publius">
                     <expan>
                        <abbr>P</abbr><ex>ublius</ex>
                     </expan>
                  </name>
                  <name type="gentilicium" key="Aelius">Aelius</name>
                  <name type="cognomen" key="Lucius">Lucius</name>
               </persName> miles <lb n="3"/>
               <expan>
