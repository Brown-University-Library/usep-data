               <persName>
                  <name type="slaveName" key="Icane">
                     <supplied reason="lost" evidence="previouseditor">I</supplied><unclear>c</unclear>ane</name>
               </persName>
               <expan>
                  <abbr>ver</abbr><ex>na</ex>
               </expan>
               <lb n="5"/>
                   <expan><abbr><supplied reason="lost">b</supplied></abbr><ex>ene</ex></expan>
               <expan>
                  <abbr><unclear>m</unclear></abbr><ex>erenti</ex>
