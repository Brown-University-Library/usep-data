                  </name> 
                  <expan>
                     <abbr>lib</abbr><ex>ertae</ex>
                  </expan>
                    <lb n="4"/> 
                  <name type="cognomen" key="Epiphania">Epi<hi rend="ligature">ph</hi>aniae</name></persName>,
                    <lb n="5"/> et su<hi rend="tall">i</hi>s omnibus.
                    <lb n="6"/> 
               <expan>
                  <abbr>H</abbr><ex>oc</ex>
               </expan> 
               <expan>
                  <abbr>m</abbr><ex>onumentum</ex>
               </expan> 
