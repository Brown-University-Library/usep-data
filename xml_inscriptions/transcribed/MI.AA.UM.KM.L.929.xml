                  <name type="cognomen" key="Phileros">
                     <hi rend="ligature">Ph</hi>ileros</name></persName>,
                    <lb n="2"/> 
                   <persName><name type="cognomen" key="Pharita">Phar<hi rend="tall">i</hi>ta</name>
