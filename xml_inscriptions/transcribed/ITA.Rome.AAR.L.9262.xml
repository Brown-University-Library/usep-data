   <text>
      <body>
         <div type="edition"><!-- transcription goes here -->
            <ab>
               <lb n="1"/>
               <expan><abbr>D</abbr><ex>is</ex></expan>
               <expan><abbr>M</abbr><ex>anibus</ex></expan>.
                <lb n="2"/>
               <persName>
                  <name type="cognomen" key="Irenia">Irenia</name>
               </persName>
               <lb n="3"/>in pace <g type="bird"/>
               <lb n="4"/>pos<surplus>u</surplus>uit sum <g type="interpunct"/>
               <num value="32" type="cardinal">XXXII</num>
               <gap reason="illegible"
                     quantity="4"
                     unit="character"
                     precision="low"/>
            </ab>
         </div>
         <div type="translation">
            <p/>
         </div>
         <div type="commentary"><!-- notes go here -->
